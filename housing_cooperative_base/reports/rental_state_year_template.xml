<odoo>
    <template id="report_rental_state_year_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Rental state</h2>
                        <p>Rental state in <strong><span t-field="o.year"/></strong>.</p>
                        <p>Document created on <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%d-%m-%Y')"/>.</p>
                        <table class="table table-condensed">
                            <tr t-foreach="o.lease_line_ids" t-as="l">
                                <t t-set="o_data" t-value="o.get_data()"/>
                                <t t-set="data_details" t-value="o_data['data_details']"/>
                                <t t-esc="data_details"/>
                            </tr>
                        </table>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
